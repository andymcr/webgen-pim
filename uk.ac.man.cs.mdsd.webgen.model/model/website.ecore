  <eClassifiers xsi:type="ecore:EClass" name="LocalAuthenticationSystem" eSuperTypes="#//Authentication">
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
      <details key="constraints" value="entitySourceOnlyIfNotEncapsulated authenticationKeyFromUserSource authenticationKeyRequiredAttribute captchaRequiresKeys"/>
    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL">
      <details key="authenticationKeyFromUserSource" value="not userSource.oclIsUndefined() implies&#xD;&#xA;&#x9;let features : Collection(Feature)&#xD;&#xA;&#x9;&#x9;= if userSource.oclIsTypeOf(Entity) then&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;userSource.oclAsType(Entity).features&#xD;&#xA;&#x9;&#x9;&#x9;else if userSource.oclIsTypeOf(View) then&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;userSource.oclAsType(View).features&#xD;&#xA;&#x9;&#x9;&#x9;else let entities : Sequence(Entity)&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;= userSource.oclAsType(Service).encapsulates->select(e | e.oclIsTypeOf(Entity)).oclAsType(Entity)&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;in entities->collect(e | e.features)->union(entities->collect(e | e.associationEnds))&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;->union(userSource.oclAsType(Service).encapsulates->select(v | v.oclIsTypeOf(View)).oclAsType(View)->collect(v | v.features))&#xD;&#xA;&#x9;&#x9;&#x9;endif endif&#xD;&#xA;&#x9;in features->includes(userAuthenticationKey)"/>
      <details key="authenticationKeyRequiredAttribute" value="if userAuthenticationKey.oclIsUndefined() then false else userAuthenticationKey.cardinality = Cardinality::Required endif"/>
      <details key="captchaRequiresKeys" value="useCaptcha implies not authenticates.captchaSiteKey.oclIsUndefined() and not authenticates.captchaSecretKey.oclIsUndefined()"/>
      <details key="entitySourceOnlyIfNotEncapsulated" value="userSource.oclIsTypeOf(Entity) implies userSource.oclAsType(Entity).servedBy->isEmpty()"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="userSource" ordered="false"
        lowerBound="1" eType="#//UnitSource"/>
